{
  "expo": {
    "name": "adatingapp-client",
    "slug": "adatingapp-client",
    "version": "1.0.0",
    "extra": {
      "API_URL": "http://10.127.12.80:9999"
    },

    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "adatingappclient",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    }
  }
}
